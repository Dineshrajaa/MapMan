<?xml version='1.0' encoding='utf-8'?>
<widget id="com.LogicSquare.shaadi" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MapMan</name>
    <description>
        A sample Apache Cordova application to demonstrate phonegap-maps-plugin
    </description>
    <author email="dineshrajaa.93@gmail.com" href="http://cordova.io">
        Dinesh raja
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="plugin.google.maps" spec="~1.3.9">
        <variable name="API_KEY_FOR_ANDROID" value="AIzaSyDqn5mRlt2UDkTvHcxSwJGM9kq_1fi-ZtE" />
    </plugin>
</widget>
